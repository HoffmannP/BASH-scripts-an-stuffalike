#: mysqlTunnel.py:105
#, python-format
msgid "%s already running, kill?"
msgstr "Eine Instanze von %s läuft bereits, soll sie geschlossen werden?"

#: mysqlTunnel.py:106 mysqlTunnel.py:131
msgid "exiting"
msgstr "Dieses Programm wird beendet"

#: mysqlTunnel.py:111
#, python-format
msgid "failed to kill running %s, exiting"
msgstr "Eine Instanz von %s konnte nicht geschlossen werden, beende dieses Programm"

#: mysqlTunnel.py:121
msgid "port used, can't kill"
msgstr "Der Port ist wird bereits verwendet und das verwendenende Programm kann nicht geschlossen werden"

#: mysqlTunnel.py:124
#, python-format
msgid "port used by \"%s\", wait for 5 secs?"
msgstr "Der Port wird gerade von \"%s\" verwendet, soll 5 Sekunden auf die Freigabe gewartet werden?"

#: mysqlTunnel.py:130
#, python-format
msgid "port used by \"%s\", kill?"
msgstr "Der Port wird gerade von \"%s\" verwendet, soll das verwendende Programm geschlossen werden?"

#: mysqlTunnel.py:136
msgid "failed to kill, exiting"
msgstr "Eine Instanz konnte nicht geschlossen werden, beende dieses Programm"

#: mysqlTunnel.py:142
msgid "select MySQL server"
msgstr "Wähle den MySQL Server"

#: mysqlTunnel.py:143
msgid "server"
msgstr "Server"

#: mysqlTunnel.py:146
msgid "no server selected"
msgstr "Kein Server gewählt"

#: mysqlTunnel.py:158
msgid "connection broke, reconnect?"
msgstr "Verbindung wurde unterbrochen, soll sie erneut hergestellt werden?"
